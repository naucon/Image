{
    "name": "naucon/image",
    "description": "This package provides simple image processing class for php to create and modify GIF, PNG, JPEG images.",
    "keywords": ["image", "resize", "scale", "convert", "jpeg", "jpg", "png", "gif", "gdlib", "gd", "GD library"],
    "homepage": "https://github.com/naucon/Image",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Sanzenbacher"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-gd" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {"Naucon\\Image\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Naucon\\Image\\Tests\\": "tests"}
    }
}
